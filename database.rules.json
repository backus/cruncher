{
  "rules": {
    "docs": {
      ".read": false,
      ".write": false,
      "$doc": {
        ".read": true,
        ".write": true
      }
    },
    "publishedDocs": {
      ".read": false,
      ".write": false,
      "$publishedDoc": {
        ".read": true,
        ".write": true,
        ".validate": "newData.saveKey == $publishedDoc.val().saveKey",
        "saveKey": {
          ".read": false,
          ".write": true
        }
      }
    }
  }
}
